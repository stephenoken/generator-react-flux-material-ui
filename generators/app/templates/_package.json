    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "debowerify": "^1.3.1",
    "eslint": "^1.2.1",
    "eslint-plugin-react": "^3.2.3",
    "gulp": "^3.9.0",
    "gulp-gzip": "^1.2.0",
    "gulp-mocha": "^2.1.3",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
